{
    "env": {
        "browser": true,
        "es6": true,
        "jest": true
        },
    "extends": "airbnb-base",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
        },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
        },
    "ignorePatterns": ["dist", "coverage"],
    "rules": {
        "no-restricted-syntax": "off",
        "no-prototype-builtins": "off"
        }
    }